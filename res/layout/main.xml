	android:background="#FFFFFF">
	<Button android:id="@+id/start" android:textColor="#FFFFFF"
		android:text="Get Started" android:background="#CC0033"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:textSize="40dp" android:textStyle="bold"
		android:layout_alignParentBottom="true"
		android:layout_alignParentLeft="true" android:layout_marginBottom="104dp"
